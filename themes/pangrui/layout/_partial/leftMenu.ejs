<div id="nav">
    <nav class="nav-menu">
        <a class="site-name current" href="http://www.ruizhengyun.cn/" title="<%= theme.shortName %>"><%= theme.shortName %></a>
        <a class="site-index current" href="http://www.ruizhengyun.cn/"><i class="fa fa-home"></i><span><%= __('home') %></span></a>
        <a href="/blog/archives" title="<%= __('archives')  %>"><i class="fa fa-archives"></i><span><%= __('archives')  %></span></a>
        <a href="/blog/tags" title="<%= __('tags')  %>"><i class="fa fa-tags"></i><span><%= __('tags')  %></span></a>
        <!-- custom single page of menus -->
        <% for (let index in theme.leftPagesMenu){ %>
        <% let obj = theme.leftPagesMenu[index]; %>
        <a href="/blog/<%= obj.uri %>" title="<%= obj.title %>">
            <i class="fa <%= obj.faName %>"></i>
            <span><%= obj.title %></span>
        </a>
        <% } %>
    </nav>
</div>
